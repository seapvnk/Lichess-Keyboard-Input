{
    "manifest_version": 2,
    "name": "Lichess Keyboard Input",
    "description": "Use your keyboard to play chess",
    "version": "0.0.1.1",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://lichess.org/*",
                "https://lichess.dev/*"
            ],
           "js": [
                "./code.js",                
                "./jquery-3.5.1.min.js"
            ],
           "css": ["style.css"]
        }
    ]


}